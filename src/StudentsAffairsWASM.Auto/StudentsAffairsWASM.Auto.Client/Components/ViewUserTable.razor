@namespace StudentsAffairsWASM.Auto.Client.Components

@if (selectedRole == "Student")
{
	<ViewStudentTable StudentsList="UsersList.Students"/>
}
else if (selectedRole == "Tutor")
{
	<ViewTutorTable TutorsList="UsersList.Tutors" />
}
else if (selectedRole == "Admin")
{
	<ViewAdminTable AdminsList="UsersList.Admins" />
}
